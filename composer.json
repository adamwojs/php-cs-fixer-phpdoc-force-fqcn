{
  "name": "adamwojs/php-cs-fixer-phpdoc-force-fqcn",
  "type": "library",
  "license": "GPL",
  "authors": [
    {
      "name": "Adam WÃ³js",
      "email": "adam@wojs.pl"
    }
  ],
  "require": {
    "php": "^7.1",
    "friendsofphp/php-cs-fixer": "2.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "AdamWojs\\PhpCsFixerPhpdocForceFQCN\\": "src/"
    },
    "exclude-from-classmap": [
      "/src/Tests/"
    ]
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix src -v --show-progress=estimating",
    "run-tests": "@php ./vendor/bin/phpunit -c phpunit.xml.dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
